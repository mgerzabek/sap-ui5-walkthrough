{
    "start_url": "<startUrl>",

    "_version": "1.12.0",
    "sap.app": {
        "id": "sap.ui.demo.wt",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "dataSources": {
          "invoiceRemote": {
            "uri": "https://services.odata.org/V2/Northwind/Northwind.svc/",
            "type": "OData",
            "settings": {
                "odataVersion": "2.0"
            }
          }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "sap.ui.demo.wt.view.App",
            "type": "XML",
            "id": "app"
        },
        "dependencies": {
          "minUI5Version": "1.93",
          "libs": {
            "sap.ui.core": {},
            "sap.m": {}
          }
        },
        "models": {
          "i18n": {
            "type": "sap.ui.model.resource.ResourceModel",
            "settings": {
              "bundleName": "sap.ui.demo.wt.i18n.i18n",
              "supportedLocales": [""],
              "fallbackLocale": ""
            }
          },
          "invoice": {
            "dataSource": "invoiceRemote"
          }
        },
        "routing": {
          "config": {
          "routerClass": "sap.m.routing.Router",
          "type": "View",
          "viewType": "XML",
          "path": "sap.ui.demo.wt.view",
          "controlId": "app",
          "controlAggregation": "pages"
          },
          "routes": [
          {
            "pattern": "",
            "name": "overview",
            "target": "overview"
          },
          {
            "pattern": "detail/{invoicePath}",
            "name": "detail",
            "target": "detail"
          }
          ],
          "targets": {
          "overview": {
            "id": "overview",
            "name": "Overview"
          },
          "detail": {
            "id": "detail",
            "name": "Detail"
          }
          }
        },
        "resources": {
          "css": [
            {
              "uri": "css/style.css"
            }
          ]
        }
    },

  "sap.platform.abap": {
      "_version": "1.1.0",
      "uri": "<uri>"
  },
  "sap.platform.hcp": {
      "_version": "1.1.0",
      "uri": "<uri>"
  }
} 